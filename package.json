    "chart:entities:build": "browserify -e ./app-src/entities-charts.js -t babelify > ./public/entitiescharts.js",
    "form:sentiment:build": "browserify -e ./app-src/sentiment-form.js -t babelify > ./public/sentimentform.js",
    "chart:sentiment:build": "browserify -e ./app-src/sentiment-chart.js -t babelify > ./public/sentimentchart.js"
